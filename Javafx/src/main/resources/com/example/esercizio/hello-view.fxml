           <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
                  <children>
                     <Label fx:id="welcomeText" layoutX="89.0" layoutY="67.0" prefHeight="17.0" prefWidth="25.0" />
                      <Button layoutX="77.0" layoutY="98.0" onAction="#onHelloButtonClick" text="Hello!" />
                     <CheckBox layoutX="8.0" layoutY="49.0" mnemonicParsing="false" text="CheckBox" />
                  </children>
               </AnchorPane>
         </content>
       </Tab>
       <Tab text="Untitled Tab 2">
         <content>
           <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0" />
